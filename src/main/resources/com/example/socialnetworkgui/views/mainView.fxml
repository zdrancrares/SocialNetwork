<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="626.0" prefWidth="654.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.socialnetworkgui.controller.UserController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <TableView fx:id="tableView" prefHeight="360.0" prefWidth="614.0">
        <columns>
            <TableColumn fx:id="idColumn" text="ID"/>
          <TableColumn fx:id="lastNameColumn" prefWidth="307.3999824523926" text="Nume" />
          <TableColumn fx:id="firstNameColumn" minWidth="0.0" prefWidth="307.0000305175781" text="Prenume" />
        </columns>
          <columnResizePolicy>
              <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
          </columnResizePolicy>
      </TableView>
      <AnchorPane prefHeight="124.0" prefWidth="614.0">
         <children>
            <Button onAction="#handleAddUser" layoutX="84.0" layoutY="55.0" mnemonicParsing="false" text="Salvează" />
            <Button onAction="#handleUpdateUser" layoutX="276.0" layoutY="54.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="62.0" text="Modifică" />
            <Button onAction="#handleDeleteUser" layoutX="472.0" layoutY="55.0" mnemonicParsing="false" text="Șterge" />
         </children></AnchorPane>
   </children>
</VBox>
